// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Usuario {
  id     String  @id @default(uuid())
  status Boolean @default(true)
  nome   String
  email  String
  senha  String
  cpf    String
  rg     String?

  @@map("usuarios")
}
